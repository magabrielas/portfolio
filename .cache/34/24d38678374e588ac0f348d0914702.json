,"column":21},"source":"projects/music-master/components/App.js","original":{"line":53,"column":39}},{"generated":{"line":130,"column":26},"source":"projects/music-master/components/App.js","original":{"line":53,"column":34}},{"name":"tracks","generated":{"line":130,"column":27},"source":"projects/music-master/components/App.js","original":{"line":53,"column":45}},{"generated":{"line":131,"column":0},"source":"projects/music-master/components/App.js","original":{"line":53,"column":16}},{"generated":{"line":131,"column":8},"source":"projects/music-master/components/App.js","original":{"line":45,"column":12}},{"generated":{"line":131,"column":9},"source":"projects/music-master/components/App.js","original":{"line":44,"column":8}},{"generated":{"line":132,"column":0},"source":"projects/music-master/components/App.js","original":{"line":56,"column":5}},{"name":"Component","generated":{"line":136,"column":2},"source":"projects/music-master/components/App.js","original":{"line":19,"column":18}},{"generated":{"line":137,"column":0},"source":"projects/music-master/components/App.js","original":{"line":59,"column":0}},{"name":"App","generated":{"line":143,"column":15},"source":"projects/music-master/components/App.js","original":{"line":64,"column":15}},{"generated":{"line":144,"column":0},"source":"projects/music-master/components/App.js","original":{"line":66,"column":0}},{"generated":{"line":155,"column":0},"source":"projects/music-master/components/App.js","original":{"line":77,"column":1}}],"sources":{"projects/music-master/components/App.js":"import React, { Component } from 'react';\r\n\r\nimport Artist from './Artist';\r\nimport ArtistCard from './ArtistCard';\r\nimport Tracks from './Tracks';\r\nimport Search from './Search';\r\n\r\nimport Button from 'react-bootstrap/Button';\r\nimport Container from 'react-bootstrap/Container';\r\nimport Row from 'react-bootstrap/Row';\r\nimport Col from 'react-bootstrap/Col';\r\nimport InputGroup from 'react-bootstrap/InputGroup';\r\nimport FormControl from 'react-bootstrap/FormControl';\r\n\r\n\r\n\r\n//constante global para el url\r\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com';\r\nclass App extends Component {\r\n    state = {artist:null, tracks:[],button:false};\r\n    \r\n    searchArtist = artistQuery =>{\r\n        fetch(`${API_ADDRESS}/artist/${artistQuery}`)\r\n        .then(response => response.json())\r\n        .then(json => {\r\n            if(json.artists.total>0){\r\n                const artist=json.artists.items[0];\r\n                this.setState({artist});\r\n                fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\r\n                .then(response=> response.json())\r\n                .then(json => {\r\n                    this.setState({tracks:json.tracks})\r\n                    console.log('artist',artist);\r\n                })//fetch inferior\r\n                .catch(error => alert(error.message));\r\n            }//fin del if\r\n        })\r\n        .catch(error => alert(error.message));\r\n        \r\n    }\r\n\r\n    render(){\r\n        console.log('this.state',this.state);\r\n        return (\r\n            <Container>\r\n                <Row>\r\n                    <Col md=\"12\" align=\"center\">\r\n                        <h1>Music Master</h1>\r\n                    </Col>\r\n                </Row>\r\n                <Search searchArtist = {this.searchArtist}/>\r\n                <ArtistCard artistSearched = {this.state.artist}/>\r\n                <Tracks tracks = {this.state.tracks}/>\r\n            </Container>\r\n        )\r\n    }\r\n}\r\n\r\n/*Para tener seguimiento de lo que busca el usuario,\r\ndeclaramos una propiedad o estado 'state', inicializada\r\nen blanco'', luego se agrega un evento onChange al input\r\ndonde se llamara un metodo llamado updateArtistQuery*/\r\n\r\nexport default App;\r\n\r\n/*Declarar variable con este estilo de funcion sin estado, \r\npara usar el High-order Component, ademas de camabiar el \r\nexport default App a export default AppwithHeader\r\nconst AppWithHeader = () =>{\r\n    return(\r\n        <Header Component = {App}/>\r\n    )\r\n}\r\nexport default AppWithHeader; //Permite a otras clases importar a App sin necesidad de ponerlo en {}\r\n*/\r\n\r\n /* const bio = this.state.displayBio ? ( \r\n            <div>\r\n            <p>Hola</p>\r\n            <p>quiero avanzar rapido</p>\r\n            <p>Gracias, la gerencia</p>\r\n        </div>\r\n        ): null; //expresion ternaria\r\n        */"},"lineCount":null}},"error":null,"hash":"c56d31c6d4a44c0c33a8da70102fe9c5","cacheData":{"env":{}}}